{
  "multipart": [
    {
      "when": { "OR": [
      { "north": false, "south": false, "east": false, "west": false, "up": false, "down": false },

      { "north": true, "south": false },
      { "south": true, "north": false },
      { "up": true, "down": false },
      { "down": true, "up": false },
      { "east": true, "west": false },
      { "west": true, "east": false },

      { "north": true, "east": true },
      { "north": true, "west": true },
      { "north": true, "up": true },
      { "north": true, "down": true },
      { "east": true, "south": true },
      { "east": true, "up": true },
      { "east": true, "down": true },
      { "west": true, "up": true },
      { "west": true, "down": true },
      { "west": true, "south": true },
      { "south": true, "up": true },
      { "south": true, "down": true }
    ]},
      "apply": { "model": "xercamod:block/rope_knot" }
    },

    {
      "when": { "north": true },
      "apply": { "model": "xercamod:block/rope_straight" }
    },
    {
      "when": { "east": true },
      "apply": { "model": "xercamod:block/rope_straight", "y": 90 }
    },
    {
      "when": { "south": true },
      "apply": { "model": "xercamod:block/rope_straight", "y": 180 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "xercamod:block/rope_straight", "y": 270 }
    },
    {
      "when": { "up": true },
      "apply": { "model": "xercamod:block/rope_straight", "x": 270 }
    },
    {
      "when": { "down": true },
      "apply": { "model": "xercamod:block/rope_straight", "x": 90 }
    }
  ]
}
